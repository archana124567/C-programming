#include<stdio.h>
#include<conio.h>
int main()
{

	int marks;
	printf("Enter yours marks : ");    //asking for input of marks 
	scanf("%d",&marks);                //storing marks
	//decision making statements 
	if(marks>=85 && marks<=100)
	{
		printf("Grade A \n");
	} 
	else if(marks>=70 && marks<=84)
	{
		printf("Grade B \n");
	}
	else if(marks>=55 && marks<=69)
	{
		printf("Grade C \n");
	}
	else if(marks>=40 && marks<=54)
	{
		printf("Grade D \n");
	}
	else
	{
		printf("Grade F \n");
	}
	
	// exit status 
	return 0;
}
